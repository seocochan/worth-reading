  "version": "0.0.1",
  "description": "A browser extension that lets you quickly determine if an article is worth reading with the help of LLMs.",
  "author": "Seokho Jeong",
  "packageManager": "npm@9.5.1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.76.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  }
